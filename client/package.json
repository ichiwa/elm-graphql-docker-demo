  "dependencies": {
    "config": "^1.24.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.5.3",
    "elm-webpack-loader": "^3.1.0",
    "file-loader": "^0.9.0",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.14.0"
  },
  "devDependencies": {
