{
  "name": "elm-graphql-docker-example",
  "version": "1.0.0",
  "description": "elm graphql docker",
  "main": "index.js",
  "scripts": {
    "start:node": "`npm bin`/nodemon --exec `npm bin`/babel-node index.js",
    "build:elm": "elm-make Main.elm --output ./public/index.html",
